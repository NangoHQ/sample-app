{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev:watch": "tsc -b -w",
    "dev:start": "dotenv -e ../.env node ./dist/index.js",
    "start": "npm run db:migrate && tsc -b && npm run dev:start",
    "db:generate": "dotenv -e ../.env prisma generate",
    "db:migrate": "dotenv -e ../.env prisma db push --skip-generate"
  },
  "type": "module",
  "dependencies": {
    "@nangohq/node": "0.39.16",
    "@prisma/client": "5.12.1",
    "fastify": "4.26.2",
    "prisma": "5.12.1",
    "zod": "3.22.4"
  }
}
